<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
    <servlet>
  	<servlet-name>accueil</servlet-name>
  	<display-name>accueil</display-name>
  	<description></description>
  	<servlet-class>servlet.accueil</servlet-class>
  </servlet>
    <servlet>
    	<servlet-name>Test</servlet-name>
    	<display-name>Test</display-name>
    	<description></description>
    	<servlet-class>servlet.Test</servlet-class>
    </servlet>
    <servlet>
    	<servlet-name>Contact</servlet-name>
    	<display-name>Contact</display-name>
    	<description></description>
    	<servlet-class>servlet.Contact</servlet-class>
    </servlet>
    <servlet>
    	<servlet-name>DataServlet</servlet-name>
    	<display-name>DataServlet</display-name>
    	<description></description>
    	<servlet-class>servlet.DataServlet</servlet-class>
    </servlet>
  <servlet-mapping>
  	<servlet-name>accueil</servlet-name>
  	<url-pattern>/Bonjour</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Test</servlet-name>
  	<url-pattern>/Test</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Contact</servlet-name>
  	<url-pattern>/Contact</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DataServlet</servlet-name>
  	<url-pattern>/DataServlet</url-pattern>
  </servlet-mapping>
</web-app>
